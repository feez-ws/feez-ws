{
  "name": "feez-ws",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:ssg && npm run build:api && npm run build:ssr",
    "build:ssg": "tsc && vite build --outDir .stormkit/public",
    "build:ssr": "tsc && vite build -c vite.config.ssr.ts && cp index.html .stormkit/server/ && rm -rf .stormkit/server/*.png",
    "build:api": "rm -rf .stormkit/api && node --loader ts-node/esm vite.config.api.ts && rm -rf .stormkit/api/*.svg",
    "preview": "vite preview",
    "server": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm' src/vite-server.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@stormkit/serverless": "^0.15.0",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
